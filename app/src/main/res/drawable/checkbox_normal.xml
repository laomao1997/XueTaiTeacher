<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke
                android:width="2px"
                android:color="@color/colorGray" />

            <padding
                android:bottom="4px"
                android:left="4px"
                android:right="4px"
                android:top="4px" />

            <size android:width="12dp"
                android:height="12dp"/>

        </shape>
    </item>
</layer-list>